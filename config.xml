<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dannydoku.digitalpool" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DannyDokuDigitalPool</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^7.0.0" />
    <engine name="browser" spec="^5.0.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="com.wmjalak.cordova.fileopener" spec="https://github.com/wmjalak/cordova-plugin-file-opener" />
    <plugin name="cordova-plugin-fileopener" spec="^1.0.5" />
    <plugin name="cordova-plugin-fastrde-downloader" spec="git+https://github.com/fastrde/phonegap-downloader.git" />
    <plugin name="cordova-plugin-downloader2" spec="0.0.6" />
    <plugin name="org.apache.cordova.file" />
    <plugin name="cordova-plugin-music-controls" spec="git+https://github.com/homerours/cordova-music-controls-plugin.git" />
    <plugin name="cordova-plugin-file-opener2" spec="^2.0.19" />
    <plugin name="cordova-plugin-media" spec="^5.0.2">
        <variable name="KEEP_AVAUDIOSESSION_ALWAYS_ACTIVE" value="NO" />
    </plugin>
    <plugin name="cordova-plugin-downloader" spec="0.0.3" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="nl.x-services.plugins.backgroundaudio" spec="^1.0.1" />
</widget>
